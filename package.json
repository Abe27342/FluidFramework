{
	"name": "@intentional/shared-tree",
	"version": "0.14.0",
	"description": "Microsoft Whiteboard collaboration libraries",
	"bugs": {
		"url": "https://dev.azure.com/intentional/intent/_workitems"
	},
	"repository": {
		"type": "git",
		"url": "https://dev.azure.com/intentional/intent/_git/whiteboard-collaboration"
	},
	"license": "MIT",
	"author": "Microsoft",
	"sideEffects": false,
	"main": "dist/index.js",
	"module": "lib/index.js",
	"types": "dist/index.d.ts",
	"scripts": {
		"build": "concurrently npm:build:compile && npm run build:docs",
		"build:compile": "concurrently npm:tsc npm:build:esnext",
		"build:docs": "api-extractor run --local && copyfiles -u 2 ./_api-extractor-temp/doc-models/* ../../api-metadata/",
		"build:esnext": "tsc --project ./tsconfig.esnext.json",
		"build:full": "npm run build",
		"build:full:compile": "npm run build:compile",
		"clean": "rimraf dist lib *.tsbuildinfo *.build.log",
		"eslint": "eslint src",
		"eslint:fix": "eslint src --fix",
		"lint": "npm run eslint",
		"lint:fix": "npm run eslint:fix",
		"test": "npm run test:mocha",
		"test:mocha": "mocha \"dist/**/*.tests.js\" --config ../../.mocharc.js",
		"tsc": "tsc --pretty"
	},
	"dependencies": {
		"@fluidframework/common-utils": "^0.25.0",
		"@fluidframework/container-definitions": "^0.31.1",
		"@fluidframework/core-interfaces": "^0.31.1",
		"@fluidframework/datastore-definitions": "^0.31.1",
		"@fluidframework/protocol-definitions": "^0.1016.1",
		"@fluidframework/shared-object-base": "^0.31.1",
		"@fluidframework/telemetry-utils": "^0.31.1",
		"buffer": "^6.0.2",
		"immutable": "^4.0.0-rc.12",
		"sorted-btree": "^1.4.0",
		"uuid": "^8.3.1"
	},
	"devDependencies": {
		"@fluidframework/aqueduct": "^0.31.1",
		"@fluidframework/build-common": "^0.19.2",
		"@fluidframework/container-definitions": "^0.31.1",
		"@fluidframework/container-loader": "^0.31.1",
		"@fluidframework/eslint-config-fluid": "^0.21.0",
		"@fluidframework/local-driver": "^0.31.1",
		"@fluidframework/runtime-definitions": "^0.31.1",
		"@fluidframework/runtime-utils": "^0.31.1",
		"@fluidframework/server-local-server": "^0.1016.1",
		"@fluidframework/test-runtime-utils": "^0.31.1",
		"@fluidframework/test-utils": "^0.31.1",
		"@typescript-eslint/parser": "^4.7.0",
		"chai": "^4.2.0",
		"concurrently": "^5.2.0",
		"easy-table": "^1.1.1",
		"env-cmd": "^10.1.0",
		"rimraf": "^2.6.2"
	},
	"publishConfig": {
		"registry": "https://pkgs.dev.azure.com/intentional/_packaging/intentional-npm/npm/registry/"
	}
}
